{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://api.maas.global/maas-backend/profile/profile-devices-put/request",
  "description": "Request schema for profile-active-plan-put",
  "type": "object",
  "properties": {
    "identityId": {
      "$ref": "../../../core/aws-units.json#/definitions/identityId"
    },
    "payload": {
      "type": "object",
      "properties": {
        "devicePushToken": {
          "type": "string"
        },
        "deviceIdentifier": {
          "type": "string"
        },
        "deviceType": {
          "type": "string",
          "enum": ["iOS", "Android"]

        }
      },
      "required": ["devicePushToken", "deviceIdentifier"]
    }
  },
  "additionalProperties": false
}
