{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://api.maas.global/core/aws-units",
  "description": "AWS common units",
  "definitions": {
    "arn": {
      "type": "string",
      "pattern": "^arn:aws:[a-z]+:[\\w\\-]*:\\d{12}:[ \\w\\-]+(\\/[\\w\\- \\/]+)*",
      "minLength": 1,
      "maxLength": 256
    },
    "identityId": {
      "type": "string",
      "pattern": "^[aepus]{2}-[\\w]{4}-\\d:[a-f\\d]{8}(-[a-f\\d]{4}){3}-[a-f\\d]{12}$"
    },
    "deviceToken": {
      "type": "string",
      "pattern": "^([A-Fa-f0-9]{2}){8,64}$"
    }
  }
}
