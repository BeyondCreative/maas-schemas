{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://api.maas.global/maas-backend/itineraries/itinerary-retrieve/request",
  "description": "Request schema for itinerary-retrieve",
  "type": "object",
  "properties": {
    "itineraryId": {
      "description": "Universally unique identifier, see https://en.wikipedia.org/wiki/Universally_unique_identifier",
      "type": "string",
      "pattern": "^[A-Fa-f0-9]{8}(-[A-Fa-f0-9]{4}){3}-[A-Fa-f0-9]{12}$"
    }
  },
  "required": [
    "itineraryId"
  ],
  "additionalProperties": false
}